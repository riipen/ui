{
  "name": "riipen-ui-docs",
  "version": "0.0.1",
  "author": {
    "name": "Riipen",
    "email": "admin@riipen.com",
    "url": "https://riipen.com",
    "directory": "packages/riipen-ui-docs"
  },
  "license": "MIT",
  "repository": "https://github.com/riipen/ui/packages/riipen-ui-docs",
  "private": true,
  "engines": {
    "npm": ">=7.5.0"
  },
  "scripts": {
    "build": "npm run build:api && npm run build:next",
    "build:api": "BABEL_ENV=test babel-node ./scripts/buildAPI",
    "build:next": "NODE_ENV=production next build",
    "export": "NODE_ENV=production next export",
    "format": "prettier --write \"**/*.{js,jsx}\"",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "server": "next start",
    "start": "next"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "peerDependencies": {
    "draft-js": ">= 0.11.6",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-jsx": ">= 3.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "babel-plugin-module-resolver": "^3.2.0",
    "clsx": "1.0.4",
    "doctrine": "^3.0.0",
    "lodash": "4.17.19",
    "marked": "^0.7.0",
    "next": "10.0.4",
    "prismjs": "^1.21.0",
    "prop-types": "15.7.2",
    "raw-loader": "^3.1.0",
    "react-docgen": "^5.3.1",
    "recast": "^0.18.5",
    "riipen-ui": "^0.1.24"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.6.3",
    "chokidar": "^3.4.3",
    "eslint": "6.5.1",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2"
  }
}
