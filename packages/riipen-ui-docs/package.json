{
  "name": "riipen-ui-docs",
  "version": "0.0.1",
  "author": {
    "name": "Riipen",
    "email": "admin@riipen.com",
    "url": "https://riipen.com",
    "directory": "packages/riipen-ui-docs"
  },
  "repository": "https://github.com/riipen/ui",
  "private": true,
  "engines": {
    "npm": ">=6.12.0"
  },
  "scripts": {
    "build": "npm run build:api && npm run build:next",
    "build:api": "BABEL_ENV=test babel-node ./scripts/buildAPI",
    "build:next": "NODE_ENV=production next build",
    "ci:audit": "audit-ci --moderate",
    "export": "NODE_ENV=production next export",
    "format": "prettier --write \"**/*.{js,jsx}\"",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "server": "next start",
    "start": "next"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": ">= 16.11.0",
    "react-dom": ">= 16.11.0",
    "styled-jsx": ">= 3.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "babel-plugin-module-resolver": "^3.2.0",
    "clsx": "1.0.4",
    "doctrine": "^3.0.0",
    "draft-convert": "^2.1.10",
    "draft-js": "^0.11.5",
    "lodash": "4.17.15",
    "marked": "^0.7.0",
    "next": "^9.1.5",
    "prismjs": "^1.17.1",
    "prop-types": "15.7.2",
    "raw-loader": "^3.1.0",
    "react-docgen": "^4.1.1",
    "recast": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "audit-ci": "2.4.2",
    "eslint": "6.5.1",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2"
  }
}
